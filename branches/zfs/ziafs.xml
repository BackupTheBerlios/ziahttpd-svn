<system>
  <nb_thread>10</nb_thread>
  <poll_delay>1000000</poll_delay>
  <expiration_delay>10</expiration_delay>
  <nr_bklog>10</nr_bklog>
</system>

<server>
  <id>1</id>
  <cgi_id>1</cgi_id>
  <port>40000</port>
  <directory_index>index.html index.htm</directory_index>
</server>


<directory id=1>
  <servername>*</servername>
  <docroot>./</docroot>
</directory>

<mime extension="html">
  <is_cgi>0</is_cgi>
  <type>text/html</type>
  <cgi></cgi>
  <image></image>
</mime>

<mime extension="htm">
  <is_cgi>0</is_cgi>
  <type>text/html</type>
  <cgi></cgi>
  <image></image>
</mime>

<mime extension="gif">
  <is_cgi>0</is_cgi>
  <type>image/gif</type>
  <cgi></cgi>
  <image></image>
</mime>

<mime extension="exe">
  <is_cgi>1</is_cgi>
  <type>text/html</type>
  <cgi></cgi>
  <image></image>
</mime>

