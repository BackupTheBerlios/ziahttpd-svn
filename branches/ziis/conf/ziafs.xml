<system>
  <nb_thread>20</nb_thread>
  <poll_delay>500000</poll_delay>
  <expiration_delay>5</expiration_delay>
  <nr_bklog>100</nr_bklog>
  <directory_listing>./test/cgi-bin/windows/dir_list.exe</directory_listing>
</system>


<module name="mod_sock">
<file>src/mod/sock/mod_sock.lo</file>
</module>

<module name="mod_resource">
<file>src/mod/resource/mod_resource.lo</file>
</module>


<server>
  <id>1</id>
  <cgi_id>1</cgi_id>
  <port>40000</port>
  <directory_index>index.html index.htm</directory_index>
  <modulename>mod_sock</modulename>
  <modulename>mod_resource</modulename>
</server>



<directory id=1>
  <servername>*</servername>
  <docroot>./</docroot>
</directory>

<mime extension="html">
  <is_cgi>0</is_cgi>
  <type>text/html</type>
  <cgi></cgi>
  <image></image>
</mime>

<mime extension="htm">
  <is_cgi>0</is_cgi>
  <type>text/html</type>
  <cgi></cgi>
  <image></image>
</mime>

<mime extension="gif">
  <is_cgi>0</is_cgi>
  <type>image/gif</type>
  <cgi></cgi>
  <image></image>
</mime>

<mime extension="exe">
  <is_cgi>1</is_cgi>
  <type>text/html</type>
  <cgi></cgi>
  <image></image>
</mime>

<mime extension="php">
  <is_cgi>0</is_cgi>
  <type>text/html</type>
  <cgi>./php/php-cgi.exe</cgi>
  <image></image>
</mime>
